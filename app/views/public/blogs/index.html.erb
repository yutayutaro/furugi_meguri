<h1 class="h1-text">登録店舗ブログ一覧</h1>
<table class="table">
 <thead>
  <tr>
   <th><%="作成日時"%></th>
   <th><%="タイトル"%></th>        
  </tr>
 </thead>
 <tbody>
  <tr>
    <% @blogs.each do |blog| %>
   <th><%= blog.created_at.strftime("%Y/%m/%d")%></th>
   <td><%= link_to blog_path(blog.id) do %>
       <%= blog.title%></td>
  </tr>
    <% end %>
    <% end %>
    <%=  paginate @blogs %>
 </tbody>
</table>