<h1 class="h1-text">ブログ一覧</h1>
<table class="table">
 <thead>
  <tr>
   <th><%="作成日時"%></th>
   <th><%="タイトル"%></th>     
   <th><%= ""%></th>
  </tr> 
 </thead>
 <tbody>
  <tr>
   <% @blogs.each do |blogs|%>  
   <td><%= blogs.created_at.strftime("%Y/%m/%d")%></td>
   <td><%= blogs.title %></td>
   <td><%= link_to"詳細ページへ", shop_blog_path(blogs.id) %></td>
  </tr>
<% end %>
 </tbody>
</table>
<%= paginate @blogs %>