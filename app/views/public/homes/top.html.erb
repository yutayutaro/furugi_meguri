 <%= render 'maps/google_map', maps: @maps,map: @map %>
 <% if customer_signed_in? %>
<h3 class="h3-text">ブックマーク店舗画像一覧</h3>
<table class="table">
 <thead>
  <tr>
   <th><%= "店舗名"%></th>
   <th><%= "画像"%></th>
   <th><%= "作成日時"%></th>
  </tr>
 </thead>
 <tbody>
  <% @shop_photos.each do |photo|%>
  <tr>
   <td><%=link_to store_photo_path(photo.id) do%>
       <%=photo.shop.name%></td>
       <% end %>
   <td><%=image_tag photo.image%></td>
   <td><%=photo.created_at.strftime("%Y/%m/%d")%></td>
  </tr>
  <% end %>
  <% paginate @shop_photos %>
 </tbody>
</table>
 <table>
  <tbody>
   <tr>
    <th><%= "ブックマーク店舗数数"%></th>
    <td><%= link_to stores_path do%>
         <%=@bookmarks.count%>
         <% end %>
         <%="店舗ブックマーク中"%></td>
         
   </tr>
  </tbody>
 </table>
<h3 class="h3-text">ブックマーク店舗一覧</h3>
<table class="table">
 <thead>
  <tr>
   <th><%="店舗名"%></th>
   <th><%="住所"%></th>
  </tr>
 </thead>
 <tbody>
  <tr>
   <% @bookmarks.each do |stores|%>
   <td><%= link_to store_path(stores.id) do%>
       <%= stores.shop.name%></td>
   <td><%= stores.shop.address%></td>
  </tr>
       <% end %>
       <% end %>
       <%= paginate @bookmarks %>
 </tbody>
</table>
<h3 class="h3-text">ブックマーク店舗ブログ一覧</h3>
<table class="table">
 <thead>
  <tr>
   <th><%="作成日時"%></th>
   <th><%="タイトル"%></th>
   <th><%="店舗名"%></th>  
  </tr>
 <tbody>
  <tr>
   <% @blog.each do |blog|%>
   <th><%= blog.created_at.strftime("%Y/%m/%d")%></th>
   <td><%= link_to blog_path(blog.id) do%>
       <%= blog.title%></td>
       <% end %>
   <td><%= link_to store_path(blog.id) do%>
       <%= blog.shop.name%></td> 
       <% end %>
  </tr>
       
       <% end %>
       <%= paginate @blog %>
 </tbody>
</table>

<% else %>
<% end %>