<% if @photo.errors.any? %>
    <div class="alert">
        <ul>
            <% @photo.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
        </ul>
    </div>
<% end %>
<h1 class="h1-text">店舗画像投稿画面</h1>
<table class="table">
 <tbody>
<%= form_with model: @photo ,url: shop_shop_photos_path, method: :post do |shop_photo| %>
  <tr>
      <th><%=shop_photo.label :image,"投稿画像"%></th>
      <td><%= shop_photo.file_field :image, accept: "image/*" %></td>
  </tr>
  <tr>
      <th><%= shop_photo.label :introduction,"説明文" %></th>
      <td><%= shop_photo.text_area :introduction %></td>
  </tr>
 </tbody>
</table>
 <%= shop_photo.submit '店舗画像を投稿する',class: "btn btn-success"%>
<% end %>