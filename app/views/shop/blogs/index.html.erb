<h1 class="h1-text">ブログ一覧</h1>
<table class="table">
 <thead>
  <tr>
   <th><%="作成日時"%></th>
   <th><%="タイトル"%></th>        
  </tr> 
 </thead>
 <tbody>
  <tr>
   <% @blogs.each  do |blogs|%>  
   <td><%= blogs.created_at.strftime("%Y/%m/%d")%></td>
   <td><%= link_to shop_blog_path(blogs.id) do %>
       <%= blogs.title %></td>
  </tr>
<% end %>
<% end %>
<%= paginate @blogs %>
 </tbody>
</table>